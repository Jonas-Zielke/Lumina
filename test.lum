# Berechnung der Fibonacci-Zahl
def fib(n):
    if n <= 1:
        return n
    else:
        return fib(n - 1) + fib(n - 2)

# Ausgabe der 10. Fibonacci-Zahl
print("Die 10. Fibonacci-Zahl ist:")
print(fib(10))

